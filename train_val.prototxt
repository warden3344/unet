    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 256
    pad: 0
    kernel_size: 3
    weight_filler{
      type:"msra"
      std:0.01
    }
  }
}
layer{
  name:"relu5"
